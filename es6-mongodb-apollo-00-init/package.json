{
  "name": "es6-mongodb-apollo-00-init",
  "version": "latest",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint --config .eslintrc.yml --fix './src/**/*.js'",
    "start": "babel-node src/index.js",
    "dev": "nodemon --exec 'babel-node src/index.js'",
    "docker:build": "docker-compose --file docker-compose.yml up --build --detach",
    "docker:exec": "docker exec --interactive=true --tty=true mongo-db /bin/bash"
  },
  "dependencies": {
    "apollo-server": "latest",
    "dotenv": "latest",
    "graphql": "latest",
    "graphql-tools": "latest",
    "mongodb": "latest",
    "uuid": "latest"
  },
  "devDependencies": {
    "@babel/cli": "latest",
    "@babel/core": "latest",
    "@babel/node": "latest",
    "@babel/preset-env": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-simple-import-sort": "latest",
    "nodemon": "latest",
    "prettier": "latest",
    "rimraf": "latest"
  }
}
