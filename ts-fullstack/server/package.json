{
  "name": "server",
  "version": "latest",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint --config .eslintrc.yml --fix './src/**/*.ts'",
    "dev": "nodemon 'src/index.ts' --exec 'ts-node' src/index.ts -e ts,graphql",
    "build": "npm run clean && tsc -p . && rsync -av --progress src/graphql/typeDefs/* dist/graphql/typeDefs/ --exclude {'*.ts'}",
    "start": "npm run build && node 'dist/index.js'",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "latest",
    "express": "latest",
    "graphql": "latest",
    "graphql-iso-date": "latest",
    "graphql-tools": "latest",
    "lodash": "latest",
    "typescript": "latest"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "latest",
    "@graphql-codegen/typescript": "latest",
    "@graphql-codegen/typescript-operations": "latest",
    "@graphql-codegen/typescript-resolvers": "latest",
    "@types/express": "latest",
    "@types/graphql": "latest",
    "@types/lodash": "latest",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-simple-import-sort": "latest",
    "graphql-import-node": "latest",
    "nodemon": "latest",
     "prettier": "latest",
    "rimraf": "latest",
    "ts-node": "latest"
  }
}
