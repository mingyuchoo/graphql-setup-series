{
  "name": "demo",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "description": "",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint --config .eslintrc.yml --fix './src/**/*.ts'",
    "start": "npm run build && node dist/server",
    "build": "npm -s run clean && npm -s run generate && npm -s run lint && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/nexusSchema",
    "create-migration": "prisma migrate dev --preview-feature",
    "migrate-database": "prisma db push --preview-feature",
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server"
  },
  "dependencies": {
    "@nexus/schema": "0.20.1",
    "@paljs/nexus": "2.9.1",
    "@prisma/client": "2.13.1",
    "apollo-server": "2.19.0",
    "graphql": "15.4.0"
  },
  "devDependencies": {
    "@prisma/cli": "2.13.1",
    "@types/jest": "^26.0.19",
    "@types/node": "14.14.14",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^8.10.2",
    "ts-node-dev": "1.1.1",
    "typescript": "^4.1.3"
  },
  "repository": "",
  "author": ""
}
