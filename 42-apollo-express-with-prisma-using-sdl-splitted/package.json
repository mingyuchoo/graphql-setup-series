{
  "name": "42-apollo-express-with-prisma-using-sdl-splitted",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint --config .eslintrc.yml --fix './src/**/*.ts'",
    "dev": "nodemon 'src/index.ts' --exec 'ts-node' src/index.ts -e ts,graphql",
    "build": "npm run clean && tsc -p . && rsync -av --progress src/graphql/typeDefs/* dist/graphql/typeDefs/ --exclude {'*.ts'}",
    "start": "npm run build && node 'dist/index.js'",
    "prisma:introspect": "npx prisma introspect",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate:save": "npx prisma migrate save --experimental",
    "prisma:migrate:up": "npx prisma migrate up --experimental --verbose",
    "prisma:studio": "npx prisma studio --experimental",
    "prisma:seed": "ts-node prisma/seed.ts",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "1.19.0",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-resolvers": "1.17.10",
    "@prisma/cli": "2.10.0",
    "@types/express": "^4.17.8",
    "@types/graphql": "^14.5.0",
    "@types/lodash": "^4.14.162",
    "@types/node": "^14.14.5",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "apollo": "^2.31.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "graphql-import-node": "^0.0.4",
    "lodash": "^4.17.20",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@prisma/client": "2.10.0",
    "apollo-server-express": "^2.18.2",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-tools": "^7.0.1"
  }
}
