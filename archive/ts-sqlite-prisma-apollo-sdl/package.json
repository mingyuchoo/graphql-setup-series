{
  "name": "back-end",
  "version": "latest",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint --config .eslintrc.yml --fix './src/**/*.ts'",
    "dev": "nodemon 'src/index.ts' --exec 'ts-node' src/index.ts -e ts,graphql",
    "build": "npm run clean && tsc -p . && cp -r src/schema dist/schema",
    "start": "npm run build && node 'dist/index.js'",
    "prisma:introspect": "npx prisma introspect",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate:save": "npx prisma migrate save --experimental",
    "prisma:migrate:up": "npx prisma migrate up --experimental",
    "prisma:studio": "npx prisma studio --experimental",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.5",
    "@graphql-codegen/typescript": "^2.8.8",
    "@graphql-codegen/typescript-operations": "^2.5.13",
    "@graphql-codegen/typescript-resolvers": "^2.7.13",
    "@prisma/cli": "4.4.0",
    "@types/express": "^4.17.16",
    "@types/graphql": "^14.5.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "apollo": "^2.34.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "graphql-import-node": "^0.0.5",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "4.9.0",
    "apollo-server-express": "^3.10.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.20.1",
    "graphql-tools": "^8.3.16"
  }
}
